{
    "pinVersions": false,
    "semanticCommits": true,
    "baseBranches": [
      "master"
    ],
    "packageFiles": [
      "WORKSPACE",
      "integration/bazel/WORKSPACE",
      "package.json",
      "packages/**/package.json",
      "tools/ts-api-guardian/package.json",
      "aio/package.json"
    ],
    "packageRules": [
      {
        "packagePatterns": [
          "^@bazel/.*",
          "^build_bazel.*",
          "bazel_toolchains"
        ],
        "groupName": "bazel",
        "pinVersions": false
      }
    ]
  }
